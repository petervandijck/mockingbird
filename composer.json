{
    "name": "petervandijck/mockingbird",
    "description": "Laravel package for LlamaParse document parsing service",
    "keywords": [
        "laravel",
        "llamaparse",
        "document-parsing",
        "pdf",
        "llama-index"
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "orchestra/testbench": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "PeterVanDijck\\Mockingbird\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PeterVanDijck\\Mockingbird\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PeterVanDijck\\Mockingbird\\MockingbirdServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}